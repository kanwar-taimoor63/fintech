<!DOCTYPE html>
<html>
  <head>
    <title>Fintech</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar">
    <%= link_to image_tag("/logo.png", border: 0), {action: 'home', controller: '/pages'}, 
    class:"navbar-logo" %></nav>
    <%= render 'layouts/alerts' %>
    <%= yield %>
  </body>
</html>
